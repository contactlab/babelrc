{
  "comments": false,
  "compact": true,
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": [
          "last 2 major versions",
          // --- Exclude dead browsers
          "not dead",
          "not ie > 0",
          // --- Exclude mobile browsers
          "not Android > 0",
          "not and_chr > 0",
          "not and_ff > 0",
          "not and_qq > 0",
          "not and_uc > 0",
          "not Baidu > 0",
          "not bb > 0",
          "not ie_mob > 0",
          "not ios_saf > 0",
          "not kaios > 0",
          "not op_mini all > 0",
          "not op_mob > 0",
          "not Samsung > 0"
        ],
        "useBuiltIns": "usage",
        "corejs": "3",
        "modules": false
      }
    ]
  ],
  "plugins": ["@babel/plugin-proposal-object-rest-spread"]
}
